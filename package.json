{
  "name": "kviarte-front",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production --base-href /kviarte-front/",
    "watch": "ng build --watch --configuration development",
    "deploy": "gh-pages -d dist/kviarte-front",
    "test": "ng test",
    "build:gh": "ng build --configuration production --base-href /kviarte-front/ && powershell -NoProfile -Command \"Move-Item -Path 'dist/kviarte-front/browser/*' -Destination 'dist/kviarte-front/' -Force; Remove-Item -Recurse -Force 'dist/kviarte-front/browser'\"",
    "deploy:gh": "npm run build:gh && gh-pages -d dist/kviarte-front"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.2.0",
    "@angular/cdk": "^17.3.10",
    "@angular/common": "^17.2.0",
    "@angular/compiler": "^17.2.0",
    "@angular/core": "^17.2.0",
    "@angular/forms": "^17.2.0",
    "@angular/material": "^17.3.10",
    "@angular/platform-browser": "^17.2.0",
    "@angular/platform-browser-dynamic": "^17.2.0",
    "@angular/router": "^17.2.0",
    "@apollo/client": "^3.13.9",
    "apollo-angular": "^10.0.3",
    "graphql": "^16.11.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.2.3",
    "@angular/cli": "^17.2.3",
    "@angular/compiler-cli": "^17.2.0",
    "@types/jasmine": "~5.1.0",
    "gh-pages": "^6.3.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.3.2"
  }
}